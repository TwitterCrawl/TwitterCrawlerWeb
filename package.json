{
  "name": "twitter",
  "version": "1.0.0",
  "description": "Web interface for our twitter crawler written in node.js",
  "main": "index.js",
  "scripts": {
    "transpile": "NODE_ENV=production node_modules/.bin/webpack -p",
    "transpile-client": "babel ./src/client_components/searchEngine.js -o ./src/client_components/searchEngine.compiled.js",
    "server": "NODE_ENV=production node_modules/.bin/babel-node --presets 'react,es2015' server.js",
    "start": "npm run transpile && npm run transpile-client && npm run server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TwitterCrawl/TwitterCrawlerWeb.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TwitterCrawl/TwitterCrawlerWeb/issues"
  },
  "homepage": "https://github.com/TwitterCrawl/TwitterCrawlerWeb#readme",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "body-parser": "^1.15.2",
    "ejs": "^2.5.2",
    "express": "^4.14.0",
    "jQuery": "^1.7.4",
    "materialize-css": "^0.97.8",
    "morgan": "^1.7.0",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-router": "^3.0.0"
  },
  "devDependencies": {
    "babel-loader": "^6.2.8",
    "webpack": "^1.13.3"
  }
}
